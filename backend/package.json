{
  "name": "whatsfest-backend",
  "version": "1.0.0",
  "description": "Backend API para WhatsFest Calendar",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx prisma generate && tsc",
    "start": "node dist/server.js",
    "render-build": "npx prisma generate && npm run build",
    "render-start": "npx prisma migrate deploy && npm start",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "seed": "ts-node prisma/seed.ts",
    "db:seed": "npx prisma db seed",
    "test": "echo \"Error: no test specified\" && exit 1",
  },
  "keywords": [
    "calendar",
    "api",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
